import urllib
import urlparse

base="http://example.com/html/a.html"

print(urlparse.urljoin(base,"b.html"))
print(urlparse.urljoin(base, "sub/c.html"))
print(urlparse.urljoin(base,"../index.html"))
print(urlparse.urljoin(base,"../img/hoge.png"))
print(urlparse.urljoin(base,"../css/hoge.css"))
